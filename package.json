{
  "name": "notebook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ramda": "^0.26.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "create:pull-request": "node scripts/issueIntoPullRequest.js",
    "eject": "react-scripts eject",
    "flow": "flow",
    "format": "npm run prettier -- --write",
    "lint": "npx eslint src",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\" \"scripts/**\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run flow",
    "precommit": "npm run validate"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.109.0",
    "husky": "^3.0.9",
    "prettier": "^1.18.2"
  }
}
